cmake_minimum_required(VERSION 3.10)

find_package(Boost REQUIRED COMPONENTS stacktrace_backtrace)
find_package(GaiaBlackboards)
find_package(GaiaReflection)
find_package(GaiaExceptions)

include(@CONFIG_INSTALL_PATH@/@TARGET_NAMESPACE@@TARGET_NAME@Targets.cmake)
if(CMAKE_BUILD_TYPE STREQUAL Debug)
    include(@CONFIG_INSTALL_PATH@/@TARGET_NAMESPACE@@TARGET_NAME@Targets-debug.cmake)
endif()

find_path(@TARGET_NAMESPACE@@TARGET_NAME@_INCLUDE_DIRS "@TARGET_NAMESPACE@@TARGET_NAME@")

if (@TARGET_NAMESPACE@@TARGET_NAME@_INCLUDE_DIRS)
    set(@TARGET_NAMESPACE@@TARGET_NAME@_FOUND TRUE)
endif()